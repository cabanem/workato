# Copy to .env and fill as needed

# Root URL(s) to crawl (comma-separated allowed)
START_URLS=https://docs.workato.com/en/developing-connectors/sdk.html,https://docs.workato.com/developing-connectors/sdk/

# Where to store artifacts
DATA_DIR=./data

# Embedding model; BGE is an excellent default
EMBEDDING_MODEL=BAAI/bge-small-en-v1.5

# Enable re-ranking (optional; increases latency)
RERANK=true

# LLM provider: openai | none
LLM_PROVIDER=openai

# --- if LLM_PROVIDER=openai ---
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=  # leave empty for default
